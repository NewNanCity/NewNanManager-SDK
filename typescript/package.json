{
  "name": "@nanmanager/typescript-sdk",
  "version": "1.0.0",
  "description": "TypeScript client SDK for NewNanManager API based on @sttot/axios-api",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "nanmanager",
    "minecraft",
    "api",
    "client",
    "sdk",
    "typescript",
    "axios"
  ],
  "author": "NewNanManager Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/NewNanManager.git",
    "directory": "clients/typescript"
  },
  "bugs": {
    "url": "https://github.com/your-org/NewNanManager/issues"
  },
  "homepage": "https://github.com/your-org/NewNanManager#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "@sttot/axios-api": "0.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "axios": ">=1.0.0",
    "@sttot/axios-api": "0.2.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
